@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix cs:   <http://example.org/cs#> .

# =============================
# 计算机科学领域 RDFS 词汇
# =============================

# 顶层类
cs:AcademicEntity a rdfs:Class ;
    rdfs:label "学术实体"@zh .

cs:Person a rdfs:Class ;
    rdfs:label "人员"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Course a rdfs:Class ;
    rdfs:label "课程"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:ResearchProject a rdfs:Class ;
    rdfs:label "科研项目"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Publication a rdfs:Class ;
    rdfs:label "出版物"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

# 人员子类
cs:Teacher a rdfs:Class ;
    rdfs:label "教师"@zh ;
    rdfs:subClassOf cs:Person .

cs:Student a rdfs:Class ;
    rdfs:label "学生"@zh ;
    rdfs:subClassOf cs:Person .

# 课程子类
cs:UndergraduateCourse a rdfs:Class ;
    rdfs:label "本科课程"@zh ;
    rdfs:subClassOf cs:Course .

cs:GraduateCourse a rdfs:Class ;
    rdfs:label "研究生课程"@zh ;
    rdfs:subClassOf cs:Course .

# 属性定义

## 对象属性

cs:teaches a rdf:Property ;
    rdfs:label "教授课程"@zh ;
    rdfs:domain cs:Teacher ;
    rdfs:range  cs:Course .

cs:enrolledIn a rdf:Property ;
    rdfs:label "选修课程"@zh ;
    rdfs:domain cs:Student ;
    rdfs:range  cs:Course .

cs:supervises a rdf:Property ;
    rdfs:label "指导项目"@zh ;
    rdfs:domain cs:Teacher ;
    rdfs:range  cs:ResearchProject .

cs:memberOfProject a rdf:Property ;
    rdfs:label "参与项目"@zh ;
    rdfs:domain cs:Person ;
    rdfs:range  cs:ResearchProject .

cs:authorOf a rdf:Property ;
    rdfs:label "作者为"@zh ;
    rdfs:domain cs:Person ;
    rdfs:range  cs:Publication .

cs:relatedCourse a rdf:Property ;
    rdfs:label "相关课程"@zh ;
    rdfs:domain cs:Publication ;
    rdfs:range  cs:Course .

## 数据属性

cs:name a rdf:Property ;
    rdfs:label "名称"@zh ;
    rdfs:domain cs:AcademicEntity ;
    rdfs:range  xsd:string .

cs:personId a rdf:Property ;
    rdfs:label "人员编号"@zh ;
    rdfs:domain cs:Person ;
    rdfs:range  xsd:string .

cs:courseCode a rdf:Property ;
    rdfs:label "课程代码"@zh ;
    rdfs:domain cs:Course ;
    rdfs:range  xsd:string .

cs:credits a rdf:Property ;
    rdfs:label "学分"@zh ;
    rdfs:domain cs:Course ;
    rdfs:range  xsd:integer .

cs:year a rdf:Property ;
    rdfs:label "年份"@zh ;
    rdfs:domain cs:AcademicEntity ;
    rdfs:range  xsd:gYear .

cs:projectId a rdf:Property ;
    rdfs:label "项目编号"@zh ;
    rdfs:domain cs:ResearchProject ;
    rdfs:range  xsd:string .

cs:doi a rdf:Property ;
    rdfs:label "DOI"@zh ;
    rdfs:domain cs:Publication ;
    rdfs:range  xsd:string .

# =============================
# 校园场景扩展：类
# =============================

cs:University a rdfs:Class ;
    rdfs:label "大学"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Department a rdfs:Class ;
    rdfs:label "学院/系"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Building a rdfs:Class ;
    rdfs:label "教学楼"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Room a rdfs:Class ;
    rdfs:label "教室"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

# =============================
# 校园场景扩展：对象属性
# =============================

cs:hasDepartment a rdf:Property ;
    rdfs:label "拥有学院/系"@zh ;
    rdfs:domain cs:University ;
    rdfs:range  cs:Department .

cs:hasRoom a rdf:Property ;
    rdfs:label "拥有教室"@zh ;
    rdfs:domain cs:Building ;
    rdfs:range  cs:Room .

cs:belongsToDepartment a rdf:Property ;
    rdfs:label "隶属于学院/系"@zh ;
    rdfs:domain cs:AcademicEntity ;
    rdfs:range  cs:Department .

cs:heldInRoom a rdf:Property ;
    rdfs:label "在教室上课"@zh ;
    rdfs:domain cs:Course ;
    rdfs:range  cs:Room .

# =============================
# 校园场景扩展：数据属性
# =============================

cs:deptCode a rdf:Property ;
    rdfs:label "学院代码"@zh ;
    rdfs:domain cs:Department ;
    rdfs:range  xsd:string .

cs:roomCode a rdf:Property ;
    rdfs:label "教室编号"@zh ;
    rdfs:domain cs:Room ;
    rdfs:range  xsd:string .

cs:capacity a rdf:Property ;
    rdfs:label "容量"@zh ;
    rdfs:domain cs:Room ;
    rdfs:range  xsd:integer .

cs:gender a rdf:Property ;
    rdfs:label "性别"@zh ;
    rdfs:domain cs:Person ;
    rdfs:range  xsd:string .

cs:enrollmentYear a rdf:Property ;
    rdfs:label "入学年份"@zh ;
    rdfs:domain cs:Student ;
    rdfs:range  xsd:gYear .

# =============================
# 校园场景进一步扩展：更多类
# =============================

cs:Faculty a rdfs:Class ;
    rdfs:label "学院（大类）"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Program a rdfs:Class ;
    rdfs:label "培养方案/专业方向"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Major a rdfs:Class ;
    rdfs:label "专业"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Semester a rdfs:Class ;
    rdfs:label "学期"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:CourseOffering a rdfs:Class ;
    rdfs:label "课程开课安排"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Enrollment a rdfs:Class ;
    rdfs:label "选课记录"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Exam a rdfs:Class ;
    rdfs:label "考试"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Staff a rdfs:Class ;
    rdfs:label "职员"@zh ;
    rdfs:subClassOf cs:Person .

cs:Administrator a rdfs:Class ;
    rdfs:label "行政人员"@zh ;
    rdfs:subClassOf cs:Staff .

cs:Advisor a rdfs:Class ;
    rdfs:label "辅导员/导师"@zh ;
    rdfs:subClassOf cs:Staff .

cs:UndergraduateStudent a rdfs:Class ;
    rdfs:label "本科生"@zh ;
    rdfs:subClassOf cs:Student .

cs:GraduateStudent a rdfs:Class ;
    rdfs:label "研究生"@zh ;
    rdfs:subClassOf cs:Student .

cs:Dormitory a rdfs:Class ;
    rdfs:label "宿舍楼"@zh ;
    rdfs:subClassOf cs:Building .

cs:DormRoom a rdfs:Class ;
    rdfs:label "宿舍房间"@zh ;
    rdfs:subClassOf cs:Room .

cs:Club a rdfs:Class ;
    rdfs:label "学生社团"@zh ;
    rdfs:subClassOf cs:AcademicEntity .

cs:Library a rdfs:Class ;
    rdfs:label "图书馆"@zh ;
    rdfs:subClassOf cs:Building .

cs:Book a rdfs:Class ;
    rdfs:label "图书"@zh ;
    rdfs:subClassOf cs:Publication .

